{
    "rules": [
        {
            "expresion": "jpeg$|png$|gif",
            "folder": "Imagenes"
        },
        {
            "expresion": "zip$|rar$|gzip$",
            "folder": "Comprimidos"
        },
        {
            "expresion": "doc$|odt$|pdf$",
            "folder": "Documentos"
        },
        {
            "expresion": "xls$|xlsx$",
            "folder": "Excels"
        },
        {
            "expresion": "txt$",
            "folder": "Txt"
        },
        {
            "expresion": "exe$",
            "folder": "Ejecutables"
        },
        {
            "expresion": "mp3$|mp4$",
            "folder": "AudioVideo"
        }
    ]
}