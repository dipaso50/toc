{
    "rules": [
        {
            "expresion": "(?i)jpeg$|png$|gif$",
            "folder": "Imagenes"
        },
        {
            "expresion": "(?i)zip$|rar$|gzip$",
            "folder": "Comprimidos"
        },
        {
            "expresion": "(?i)doc$|odt$|pdf$",
            "folder": "Documentos"
        },
        {
            "expresion": "(?i)xls$|xlsx$",
            "folder": "Excels"
        },
        {
            "expresion": "(?i)txt$",
            "folder": "Txt"
        },
        {
            "expresion": "(?i)exe$",
            "folder": "Ejecutables"
        },
        {
            "expresion": "(?i)mp3$|mp4$",
            "folder": "AudioVideo"
        }
    ]
}